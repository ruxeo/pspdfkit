 <span class=3D"kc">null</span>;
<span class=3D"kd">var</span> filePicker =3D document.querySelector('<span =
class=3D"s1">input[type=3D"file"]</span>');

registerFilePicker(filePicker, <span class=3D"kd">function</span>(pdf, file=
