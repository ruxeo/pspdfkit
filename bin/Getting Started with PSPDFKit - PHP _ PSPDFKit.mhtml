</pre><pre class=3D"pbd"><span class=3D"cm">/* ./src/app.js */</span>

<span class=3D"kd">function</span> registerFilePicker(element, callback) {
  <span class=3D"kd">function</span> handler(event) {
    <span class=3D"k">if</span> (event.target.files.length =3D=3D <span cla=
ss=3D"mi">0</span>) {
      event.target.value =3D <span class=3D"kc">null</span>;
      <span class=3D"k">return</span>;
    }
    <span class=3D"kd">var</span> pdfFile =3D event.target.files[<span clas=
s=3D"mi">0</span>];
    <span class=3D"k">if</span> (pdfFile.type !=3D=3D "<span class=3D"s2">a=
pplication/pdf</span>") {
      alert("<span class=3D"s2">Invalid file type, please load a PDF.</span=
>");
      <span class=3D"k">return</span>;
    }

    <span class=3D"kd">var</span> reader =3D <span class=3D"k">new</span> F=
ileReader();
    reader.addEventListener("<span class=3D"s2">load</span>", <span class=
=3D"kd">function</span>(event) {
      <span class=3D"kd">var</span> pdf =3D event.target.result;
      callback(pdf, pdfFile);
    });
    reader.addEventListener("<span class=3D"s2">error</span>", <span class=
=3D"kd">function</span>(error) {
      alert(error.message);
    });
    reader.readAsArrayBuffer(pdfFile);
    event.target.value =3D <span class=3D"kc">null</span>;
