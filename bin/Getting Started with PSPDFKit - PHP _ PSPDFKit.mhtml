er
   def show
     @document_id =3D params['id']
<span class=3D"gi">+    @jwt =3D prepare_jwt(@document_id)
+  end
+
+  private
+
+  def prepare_jwt(document_id)
+    config =3D Rails.application.config.x.pspdfkit
